            draw_sprite_ext(spr_cursor_title, image_index, view_xview[0]+(text_xoffset-18), view_yview[0]+(view_hview[0]/2)+text_yoffset+(spacing*(i)), 1, 1, 0, c_white, 1);
    
    }

} else if (scale &gt;= 100) &amp;&amp; (display == 1) {

